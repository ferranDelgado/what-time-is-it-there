{
  "name": "What time is it there?",
  "description": "Easy timeline viewer for worldwide remote environment .",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "options_page": "options.html",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/clock_16.png",
      "32": "/images/clock_32.png",
      "128": "/images/clock_128.png"
    }
  },
  "icons": {
    "16": "/images/clock_16.png",
    "32": "/images/clock_32.png",
    "128": "/images/clock_128.png"
  }
}
